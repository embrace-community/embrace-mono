{
	"name": "embrace-mono",
	"private": true,
	"version": "0.0.0",
	"workspaces": [
		"apps/*",
		"packages/*"
	],
	"scripts": {
		"build": "turbo run build",
		"build:landing": "turbo run build --filter creator-landing",
		"dev": "turbo run dev",
		"dev:landing": "turbo run dev --filter creator-landing",
		"lint": "turbo run lint",
		"format": "prettier --write \"**/*.{ts,tsx,md}\"",
		"test": "turbo run test"
	},
	"devDependencies": {
		"eslint-config-custom": "*",
		"prettier": "latest",
		"turbo": "^1.9.3"
	},
	"engines": {
		"node": ">=14.0.0"
	}
}
